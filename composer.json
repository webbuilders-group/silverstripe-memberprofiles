{
    "name": "webbuilders-group/silverstripe-memberprofiles",
    "description": "Member registration and profile management",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "member",
        "profile",
        "registration"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        },
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "silverstripe/cms": "~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "phpcbf": "phpcbf -n src/Email/ src/Extensions/ src/Forms/ src/Model/ src/Pages/ tests/"
    },
    "suggest": {
        "symbiote/silverstripe-gridfieldextensions": "Allows drag-and-drop reordering of fields"
    },
    "extra": {
        "expose": [
            "client"
        ],
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\MemberProfiles\\": "src/",
            "WebbuildersGroup\\MemberProfiles\\Tests\\Bootstrap": "tests/Boostrap"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symbiote\\MemberProfiles\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symbiote/silverstripe-memberprofiles": "self.version",
        "silverstripe/memberprofiles": "self.version",
        "ajshort/silverstripe-memberprofiles": "self.version",
        "silverstripe-australia/memberprofiles": "self.version"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
